QMenuBar
{
    background-color: #f8f8f8;
    color: black;
    border-bottom: 1px solid #e1e1e1;
    padding-left: 15px;
    font: 13px;
}

QMenuBar::item
{
    background-color: #f8f8f8;
    color: black;
    border: 1px solid #f8f8f8;
    padding-top: 2px;
    padding-bottom: 2px;
    padding-left: 10px;
    padding-right: 10px;
    margin-top: 0px;
    margin-bottom: 0px;
}

QMenuBar::item:selected
{
    background-color: #e1e1e1;
    color: black;
    border: 1px solid #e1e1e1;
}